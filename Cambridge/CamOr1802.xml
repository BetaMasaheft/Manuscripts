                     <textLang mainLang="gez">Geez</textLang>
                  </msItem>
                  <msItem xml:id="ms_i1.4">
                     <locus from="52r" to="72r">52r-72r</locus>
                     <title>Gospel of Mark</title>
                     <textLang mainLang="gez">Geez</textLang>
                  </msItem>
                  <msItem xml:id="ms_i1.5">
                     <locus from="74r" to="112r">74r-112r</locus>
