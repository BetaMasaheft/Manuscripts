                     <textLang mainLang="gez">Geez</textLang>
                  </msItem>
                  <msItem xml:id="ms_i1.6">
                     <locus from="113v" to="142r">113v-142r</locus>
                     <title>Gospel of John</title></msItem>
                  </msItem>
