                        <msContents>
                            <summary/>
                            <msItem xml:id="p13_i1">
                                <locus from="16r" to="26r"/>
                                <title>Notes by Stephan Wright</title>
                                <note>Notes written probably by <persName ref="PRS10170Wright">Stephen Wright</persName> 
                                    (cf. <ref target="p11_i1"/>), containing a translation of the beginning of the Amharic document entitled 'The Teaching of the
                                    Abyssinian Church'.</note>
                                <msItem xml:id="p13_i1.1">
                                    <locus target="#16r"/>
                                    <title>Beginning of the list of sixty questions</title>
                                    <note>Containing six lines</note>
                                </msItem>
                                <msItem xml:id="p13_i1.2">
                                    <locus target="#17v"/>
                                    <title>Two Amharic words</title>
