                        <objectDesc form="Leporello">
                            <supportDesc>
                                <support>
                                    <material key="paper"/>
                                    <note>Oriental paper.</note>
                                </support>
                                <extent>
                                    <note>The leporello consists of <measure
                                        unit="pages">8</measure> pages created through folding.</note>
                                    <dimensions type="outer" unit="mm">
                                        <height>615</height>
