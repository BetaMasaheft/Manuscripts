                    <physDesc>
                        <objectDesc form="Leporello">
                            <supportDesc>
                                <support>
                                    <material key="paper"/>
                                    <note>Oriental paper.</note>
                                </support>
                                <extent>
                                    <note>The leporello consists of <measure
                                        unit="leaf">8</measure> leaves created through folding.</note>
                                    <dimensions type="outer" unit="mm">
                                        <height>615</height>
                                        <width>90</width>
                                    </dimensions>
                                    <dimensions type="leaf" unit="mm">
                                        <height>90</height>
                                        <width>77</width>
                                    </dimensions>
                                </extent>
                                <condition key="other">The folds between leaves are torn in several
                                    places.</condition>
                            </supportDesc>
                        </objectDesc>
                        
                        <decoDesc>
                            <decoNote xml:id="d1" type="miniature">
                                <locus target="#1r"/>
                                <desc><ref type="authFile" corresp="AT1012Equestrian"/>:
                                    <persName ref="PRS4585Georgeo"/>.</desc>
                                <q xml:lang="gez">ሥዕለ፡ ቅዱስ፡ ጊዮርጊስ፡ ሰማዕት።</q>
                            </decoNote>
                            
                            <decoNote xml:id="d2" type="miniature">
                                <locus target="#2r"/>
                                <desc>A representation of the <ref type="authFile" corresp="AT1031VirginAndChild"/>.
                                    <persName ref="PRS6819Mary"/> with the infant <persName ref="PRS5684JesusCh"/>
                                    (identified by the caption
                                    <foreign xml:lang="gez">ምስለ፡ ፍቁር፡ ወልዳ፡</foreign>)
                                    is surrounded by the two <term key="angel">archangels</term>
                                    <persName ref="PRS7049Michael"/>  (identified by the caption
                                    <foreign xml:lang="gez">ሚካኤል፡</foreign>) and <persName
                                        ref="PRS4377Gabriel"/>  (identified by the caption
                                    <foreign xml:lang="gez">ገብርኤል፡</foreign>).</desc>
                            </decoNote>
                            
                            <decoNote xml:id="d3" type="miniature">
                                <locus target="#3r"/>
                                <desc>A representation of the <ref type="authFile"
                                    corresp="AT1055ManofSorrows"/>:
                                the upper body of Christ crowned with the crown of thorns.</desc>
                                <q xml:lang="gez">ሥዕለ፡ ኵርዓተ፡ ርእሱ።</q>
                            </decoNote>
                            
                            <decoNote xml:id="d4" type="miniature">
                                <locus target="#4r"/>
                                <desc>A representation of the <ref type="authFile"
                                    corresp="AT1005Crucifixion"/> with <persName ref="PRS6819Mary"/>
                                    to the left  (identified by the caption
                                    <foreign xml:lang="gez">ማርያም።</foreign>)
                                    and <persName ref="PRS5695John"/> to the right  (identified by the caption
                                    <foreign xml:lang="gez">ዮሐንስ።</foreign>).</desc>
                                <q xml:lang="gez">ሥዕለ፡ ስቅለት።</q>
                            </decoNote>
                            
                            <decoNote xml:id="d5" type="miniature">
                                <locus target="#5r"/>
                                <desc>A representation of the <ref type="authFile"
                                    corresp="AT1049Resurrection"/>.</desc>
                                <q xml:lang="gez">ሥዕለ፡ ትንሣኤ፡</q>
                            </decoNote>
                            
                            <decoNote xml:id="d6" type="miniature">
                                <locus target="#6r"/>
                                <desc>A representation of the <ref type="authFile" corresp="AT1036MercyCovenant"/>. 
                                </desc>
                                <q xml:lang="gez">ዘከመ፡ ወሀባ፡ ኪዳነ፡ ምሕረት፡ ለእሙ።</q>
                            </decoNote>
                            
                            <decoNote xml:id="d7" type="miniature">
                                <locus target="#6r"/>
                                <desc>A  <term key="donordepiction">donor depiction</term>: 
                                    <persName ref="PRS15008HabtaM" role="owner"/> places himself under the
                                    protection of <persName ref="PRS6819Mary"/>.</desc>
                                <q xml:lang="gez">ዘከመ፡ ተማኅፀነ፡ <persName
                                    ref="PRS15008HabtaM"><roleName type="title">አባ፡</roleName> ሀብተ፡ ሚካኤል።</persName></q>
                            </decoNote>
                            
                            <decoNote xml:id="d8" type="miniature">
                                <locus target="#7r"/>
                                <desc>A representation of an <ref type="authFile"
                                    corresp="AT1082Angel"/>: <persName ref="PRS7049Michael"/>.</desc>
                                <q xml:lang="gez">ሥዕለ፡ ቅዱስ፡ ሚካኤል፡ ሊቀ፡ መላእክት።</q>
                            </decoNote>
                            
                            <decoNote xml:id="d9" type="miniature">
                                <locus target="#8r"/>
                                <desc>A representation of an <ref type="authFile"
                                    corresp="AT1041HolyMen"/> showing <persName
                                        ref="PRS4306GabraMa"/>  (identified by the caption
                                    <foreign xml:lang="gez">አቡነ፡ ገብረ፡ መንፈስ፡ ቅዱስ።</foreign>), 
                                    <persName
                                        ref="PRS9162TaklaHa"/>  (identified by the caption
                                    <foreign xml:lang="gez">አቡነ፡ ተክለ፡ ሃይማኖት።</foreign>), <persName
                                        ref="PRS3934Ewostate"/>  (identified by the caption
                                    <foreign xml:lang="gez">አቡነ፡
                                        ኤዎ<supplied reason="omitted">ስ</supplied>ጣቴዎስ።</foreign>),
                                    <persName ref="PRS4107Fileppos"/>  (identified by the caption
                                    <foreign xml:lang="gez">አቡነ፡ ፊልጶስ። </foreign>) and 
                                    <persName ref="PRS10371Yohanne"/>  (identified by the caption
                                    <foreign xml:lang="gez">አቡነ፡ ዮሐንስ፡</foreign>).</desc>
                            </decoNote>
                        </decoDesc>
                        
                        <additions>
                            <list>
                                <item xml:id="e1">
                                    <desc>The manuscript was probably produced for
                                        <persName ref="PRS15008HabtaM" role="owner"/> who is
                                        mentioned in <ref target="#d7"/>.</desc>
                                </item>
                                
                                <item xml:id="e2">
                                    <desc>The verso is empty.</desc>
                                </item>
                                
                                <item xml:id="e3">
                                    <locus target="#1v"/>
                                    <desc type="StampExlibris"/>
                                    <q xml:lang="la">Ex libris <persName ref="PRS3182ContiRo"
                                        >CCR</persName></q>
                                </item>
                            </list>
                        </additions>
                        
                        <bindingDesc>
                            <binding xml:id="binding">
                                <decoNote xml:id="b1">Thick leather boards with a thread.</decoNote>
                                <decoNote xml:id="b2" type="bindingMaterial">
                                    <material key="leather"/>
                                </decoNote>
                                <decoNote xml:id="b3" type="SlipCase">Kept in a small bag made of rough
                                cloth, which in its turn is kept in a very damaged leather case that
                                had been made for a smaller manuscript.</decoNote>
                            </binding>
                        </bindingDesc>
