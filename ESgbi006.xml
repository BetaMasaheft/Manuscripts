                        <foreign xml:lang="en"/>
                     </incipit>
                     <explicit defective="false">
                        <foreign xml:lang="en"/>
                     </explicit>
                     <note anchored="true"/>
                  </msItem>
                  <msItem xml:id="coloph1" defective="false">
