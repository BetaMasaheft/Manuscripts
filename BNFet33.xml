                    <rdg resp="PRS10747Zotenbe" wit="BNFet32">ወለማርያ፡</rdg>
                    <rdg resp="PRS10747Zotenbe" xml:lang="grc">μετὰ Μαρίας</rdg>
                    
                </app>
