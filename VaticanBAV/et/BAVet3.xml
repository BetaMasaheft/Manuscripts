                        <item xml:id="a1">
                           <desc type="GuestText"/>
                           <locus target="#1a"/>
                           <desc>Names of the tribes of Israel and the Apostles descending from each of them.</desc>
                           <listBibl type="relations">
                              <bibl/>
                              <relation name="saws:isDirectCopyOf"
                                        active="BAVet3#ms_a1"
                                        passive="BAVet10#ms_a1"
                                        cert="unknown">
