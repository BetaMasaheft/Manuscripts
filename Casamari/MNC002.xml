                  <msItem xml:id="ms_i1">
                     <locus from="4r" to="113r"/>
                     <title type="complete" ref="LIT2701Dawit"/>
                     <note>
                     </note>
                     
                     <msItem xml:id="ms_i1.1">
                        <locus from="4r" to="87v"/>
                        <title type="complete" ref="LIT2000Mazmur"/>
                        <incipit xml:lang="gez">
                           <lb/>ፍካሬ፡ ዘጻድቃን፡ ወዘኃጥአን፡ መዝሙር፡ ዘዳዊት።
                           <lb/>ወብፁዕ፡ ብእሲ፡ ዘኢሖረ፡ በምክረ፡ <del rend="effaced">ረ</del>ሲአን። 
                        </incipit>
                        <explicit xml:lang="gez">
                           <lb/>ወመተርኩ፡ ርእሶ።
                           <lb/>ወአሰሰልኩ፡ ፅዕለተ፡ እምደቂቀ፡ እስራኤል።
                        </explicit>
                        <note>
                          
                        </note>
                     </msItem>
                     
                     <msItem xml:id="ms_i1.2">
                        <locus from="87v" to="96r"/>
                        <title type="complete" ref="LIT1828Mahale"/>
                        <msItem xml:id="ms_i1.2.1">
                           <locus from="87v" to="88v"/>
                           <title type="complete" ref="LIT1828Mahale#Moses1"/>
                           <note/>
                           <incipit xml:lang="gez">
                              <lb/>ማኅልየ፡ ነቢያት፡ ወጸሎቱ፡ ለሙሴ። ፩።
                              <lb/>ንሴብሕ፡ ለእግዚአብሔር፡ ስቡሕ፡ ዘተሰብሐ።
                           </incipit>
                           <explicit xml:lang="gez">
                              <lb/>ወደቂቀ፡ እስራኤልሰ፡ ኃለፉ፡ እንተ፡ የብስ፡ <add place="right">ማዕከለ፡ ባሕር።</add> 
                              <lb/>ማይሰ፡ ቆመ፡ ሎሙ፡ ዓረፍት፡ በይምን፡ <add place="right">ወዓረፍት፡ በጽግም።</add>
                           </explicit>
                        </msItem>
                        <msItem xml:id="ms_i1.2.2">
                           <locus from="88v" to="89v"/>
                           <title type="complete" ref="LIT1828Mahale#Moses2"/>
                           <note/>
                           <incipit xml:lang="gez">
                              <lb/>ጸሎተ፡ ሙሴ፡ ዘዳግም፡ ሕግ። ፪። 
                              <lb/>አጽምዕ፡ ሰማይ፡ ወእንግርከ።
                           </incipit>
                           <explicit xml:lang="gez">
                              <lb/>ወአነ፡ አቅንዖሙ፡ በዘኢኮነ፡ ሕዝብ። 
                              <lb/>ወአምዕዖሙ፡ ለሕዝብ፡ በዘኢይሌብው።
                           </explicit>
                        </msItem>
                        <msItem xml:id="ms_i1.2.3">
                           <locus from="89v" to="90v"/>
                           <title type="complete" ref="LIT1828Mahale#Moses3"/>
                           <note/>
                           <incipit xml:lang="gez">
                              <lb/>ጸሎተ፡ ሙሴ፡ ዘሣልሳይ። ፫። 
                              <lb/>እስመ፡ እሳት፡ ትነድድ፡ እመዓትየ።
                           </incipit>
                           <explicit xml:lang="gez">
                              <lb/>ወይትቤቀሎሙ፡ ለጸላእቱ። 
                              <lb/>ወያነጽሕ፡ እግዚአብሔር፡ ምድረ፡ ለሕዝቡ።
                           </explicit>
                        </msItem>
                        <msItem xml:id="ms_i1.2.4">
                           <locus from="90v" to="91r"/>
                           <title type="complete" ref="LIT1828Mahale#Hannah"/>
                           <note/>
                           <incipit xml:lang="gez">
                              <lb/>ጸሎተ፡ ሐና፡ እመ፡ ሳሙኤል፡ ነቢይ። ፬። 
                              <lb/>ጸንዓ፡ ልብየ፡ በእግዚአብሔር።
                           </incipit>
                           <explicit xml:lang="gez">
                              <lb/>ወይሁቦሙ፡ ኃይለ፡ ለነገሥትነ። 
                              <lb/>ወያሌዕል፡ ቀርነ፡ መሢሑ።
                           </explicit>
                        </msItem>
                        <msItem xml:id="ms_i1.2.5">
                           <locus from="91r" to="91v"/>
                           <title type="complete" ref="LIT1828Mahale#Hezekiah"/>
                           <note/>
                           <incipit xml:lang="gez">
                              <lb/>ጸሎተ፡ ሕዝቅያስ፡ ንጉሠ፡ ይሁዳ። ፭። 
                              <lb/>አንሰ፡ እቤ፡ በኑኃ፡ መዋዕልየ።
                           </incipit>
                           <explicit xml:lang="gez">
                              <lb/>በኵሉ፡ መዋዕለ፡ ሕይወትየ። 
                              <lb/>በቅድመ፡ ቤተ፡ እግዚአብሔር።
                           </explicit>
                        </msItem>
                        <msItem xml:id="ms_i1.2.6">
                           <locus from="91v" to="92v"/>
                           <title type="complete" ref="LIT1828Mahale#Manasseh"/>
                           <note/>
                           <incipit xml:lang="gez">
                              <lb/>ጸሎተ፡ ምናሴ፡ ነቢይ። ፮። 
                              እግዚኦ፡ አኃዜ፡ ኵሉ፡ ዓለም።
                           </incipit>
                           <explicit xml:lang="gez">
                              <lb/>እስመ፡ ኪያከ፡ ይሴብሑ፡ ኵሉ፡ ኃይለ፡ ሰማያት። 
                              <lb/>ወለከ፡ ሰብሐት፡ ለዓለም፡ አሜን።
                           </explicit>
                        </msItem>
                        <msItem xml:id="ms_i1.2.7">
                           <locus target="#92v"/>
                           <title type="complete" ref="LIT1828Mahale#Jonah"/>
                           <note/>
                           <incipit xml:lang="gez">
                              <lb/>ጸሎተ፡ ዮናስ፡ ነቢይ። ፯። 
                              <lb/>ጸራኅኩ፡ በምንዳቤየ፡ ኀበ፡ እግዚአብሔር፡ አምላኪየ።
                           </incipit>
                           <explicit xml:lang="gez">
                              <lb/>አንሰ፡ ምስለ፡ ቃለ፡ ሰብሐት፡ በተጋንዮ፡ ዕሠውዕ፡ ለከ። 
                              <lb/>መጠነ፡ ጸለይኩ፡ በሕይወትየ፡ አዓስዮ፡ ለእግዚአብሔር።
                           </explicit>
                        </msItem>
                        <msItem xml:id="ms_i1.2.8">
                           <locus from="92v" to="93v"/>
                           <title type="complete" ref="LIT1828Mahale#ThreeYouths1"/>
                           <note/>
                           <incipit xml:lang="gez">
                              <lb/>ጸሎተ፡ ዳንኤል፡ ነቢይ፡ ስብሐት፡ ዘእምአዛርያ። ፰። 
                              ይትባረክ፡ እግዚአብሔር፡ አምላከ፡ አበዊነ።
                           </incipit>
                           <explicit xml:lang="gez">
                              <lb/>ወያዕምሩ፡ ከመ፡ አንተ፡ እግዚአብሔር፡ <add place="right">አምላክ፡ ባሕቲትከ።</add> 
                              <lb/>ስቡሕ፡ በኵሉ፡ ምድር፡ ወበኵሉ፡ ዓለም።
                           </explicit>
                        </msItem>
                        <msItem xml:id="ms_i1.2.9">
                           <locus target="#93v"/>
                           <title type="complete" ref="LIT1828Mahale#ThreeYouths2"/>
                           <note/>
                           <incipit xml:lang="gez">
                              <lb/>ጸሎተ፡ ሠለስቱ፡ ደቂቅ። ፱። 
                              ይትባረክ፡ እግዚአብሔር፡ አምላከ፡ አበዊነ።
                           </incipit>
                           <explicit xml:lang="gez">
                              <lb/>ቡሩክ፡ አንተ፡ በመልዕልተ፡ ሰማያት። 
                              <lb/>ስቡሕኒ፡ አንተ፡ ወ<gap reason="lost" unit="chars" quantity="4"/>፡ አንተ፡ ለዓለም።
                           </explicit>
                        </msItem>
                        <msItem xml:id="ms_i1.2.10">
                           <locus from="93v" to="94r"/>
                           <title type="complete" ref="LIT1828Mahale#ThreeYouths3"/>
                           <note/>
                           <incipit xml:lang="gez">
                              <lb/>ኀበ፡ ባረኩ፡ አናንያ፡ ወአዛርያ፡ ወሚሳኤል። ፲። 
                              ይባርክዎ፡ ኵሉ፡ ግብረ፡ እግዚእ፡ ለእግዚአብሔር።
                           </incipit>
                           <explicit xml:lang="gez">
                              <lb/>ንባርኮ፡ ለአብ፡ ወወልድ፡ ወመንፈስ፡ ቅዱስ። 
                              <lb/>ስቡሕኒ፡ ውእቱ፡ ወልዑልኒ፡ ውእቱ፡ ለዓለም።
                           </explicit>
                        </msItem>
                        <msItem xml:id="ms_i1.2.11">
                           <locus from="94r" to="95r"/>
                           <title type="complete" ref="LIT1828Mahale#Habakkuk"/>
                           <note/>
                           <incipit xml:lang="gez">
                              <lb/>ጸሎተ፡ እንባቆም፡ ነቢይ። ፲ወ፩። 
                              እግዚኦ፡ ሰማዕኩ፡ ድምፀከ፡ ወፈራህኩ።
                           </incipit>
                           <explicit xml:lang="gez">
                              <lb/>ዘይሠርዖን፡ ለእገርየ፡ ወለፍጻሜየ። 
                              <lb/>ወዲበ፡ ልዑላት፡ ያፄዕነኒ። ከመ፡ እማዕ፡ በማኅልይሁ።
                           </explicit>
                        </msItem>
                        <msItem xml:id="ms_i1.2.12">
                           <locus from="95r" to="95v"/>
                           <title type="complete" ref="LIT1828Mahale#Isaiah"/>
                           <note/>
                           <incipit xml:lang="gez">
                              <lb/>ጸሎተ፡ ኢሳይያስ፡ ነቢይ። ፲ወ፪። 
                              <lb/>በሌሊት፡ ትገይስ፡ መንፈስየ፡ ኀበ፡ እግዚአብሔር፡ አምላኪየ።
                           </incipit>
                           <explicit xml:lang="gez">
                              <lb/>ወዕፁ፡ ኆኅተከ። ወተኃባዕ፡ ኅዳጠ፡ ምዕረ። 
                              <lb/>እስከ፡ የኃልፍ፡ መዓቱ፡ ለእግዚአብሔር።
                           </explicit>
                        </msItem>
                        <msItem xml:id="ms_i1.2.13">
                           <locus from="95v" to="96r"/>
                           <title type="complete" ref="LIT1828Mahale#Mary"/>
                           <note/>
                           <incipit xml:lang="gez">
                              <lb/>ጸሎተ፡ እግዝእትነ፡ ማርያም፡ ወላዲተ፡ አምላክ። ፲፫። 
                              <lb/>ተዓብዮ፡ ነፍስየ፡ ለእግዚአብሔር።
                           </incipit>
                           <explicit xml:lang="gez">
                              <lb/>ወ<add place="interlinear">ተ</add>ዘከረ፡ ሣህሎ፡ ዘይቤሎሙ፡ ለአበዊነ። 
                              <lb/>ለአብርሃም፡ ወለዘርኡ፡ እስለ፡ ለዓለም።
                           </explicit>
                        </msItem>
                        <msItem xml:id="ms_i1.2.14">
                           <locus target="#96r"/>
                           <title type="complete" ref="LIT1828Mahale#Zachariah"/>
                           <note/>
                           <incipit xml:lang="gez">
                              <lb/>ጸሎተ፡ ዘካርያስ፡ ነቢይ። ፲ወ፬። 
                              <lb/>ይትባረክ፡ እግዚአብሔር፡ አምላከ፡ እስራኤል።
                           </incipit>
                           <explicit xml:lang="gez">
                              <lb/>ለእለ፡ ይነብሩ፡ ውስተ፡ ጽልመት፡ ወጽላሎተ፡ ሞት። 
                              <lb/>ከመ፡ ያርትዕ፡ እግረነ፡ ውስተ፡ ፍኖተ፡ ሰላም።
                           </explicit>
                        </msItem>
                        <msItem xml:id="ms_i1.2.15">
                           <locus target="#96r"/>
                           <title type="complete" ref="LIT1828Mahale#Simeon"/>
                           <note/>
                           <incipit xml:lang="gez">
                              <lb/>ጸሎተ፡ ስምዖን፡ ነቢይ። ፲ወ፭። 
                              ይእዜ፡ ትስእሮ፡ ለገብርከ።
                           </incipit>
                           <explicit xml:lang="gez">
                              <lb/>ከመ፡ ትከስት፡ ብርሃነ፡ ለአሕዛብ። 
                              <lb/>ወክብረ፡ ለሕዝብከ፡ እስራኤል።
                           </explicit>
                        </msItem>
                     </msItem>
                     
                     <msItem xml:id="ms_i1.3">
                        <locus from="96r" to="102r"/>
                        <title type="complete" ref="LIT2362Songof"/>
                        <note/>
                        <incipit xml:lang="gez">
                           <lb/>ማኅልየ፡ ማኅልይ፡ ዝውእቱ፡ ዘሰሎሞን። ፩። 
                           ይስእመኒ፡ በስዕመተ፡ አፉሁ።
                        </incipit>
                        <explicit xml:lang="gez">
                           <lb/>ጕይ፡ አንተ፡ ወልድ፡ እኁየ፡ <add place="right">ወተመሰላ፡ ለወይጠል።</add> 
                           ወእመ፡ አኮ፡ ከመ፡ ወሬዛ፡ <add place="right">ኃይል፡ ውስተ፡ አድባረ፡ ድኁኃን።</add>
                        </explicit>
                     </msItem>
                  
                     <msItem xml:id="ms_i1.4">
                        <locus from="102va" to="109rb"/>
                        <title type="complete" ref="LIT2509Weddas"/>
                        <msItem xml:id="ms_i1.4.1">
                           <locus from="102va" to="103ra"/>
