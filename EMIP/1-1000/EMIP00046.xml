                  <decoDesc>
                    <decoNote type="miniature" xml:id="d01"><locus target="#ivr"/><desc><ref type="authFile" corresp="AT1024MosesLaw"></ref>, <persName ref = "PRS1002Aaron"/> and <persName ref = "PRS10774Paraq"/></desc></decoNote>
                    <decoNote type="miniature" xml:id="d02"><locus target="#ivv"/><desc><persName ref = "PRS7049Michael"/> enthroned <ref type="authFile" corresp="AT1082Angel"></ref></desc></decoNote>
                    <decoNote type="miniature" xml:id="d03"><locus target="#14r"/><desc>A miniature of the Baptism of Jesus <ref type="authFile" corresp="AT1038Baptism"></ref></desc></decoNote>
                    <decoNote type="miniature" xml:id="d04"><locus target="#34v"/><desc><persName ref = "PRS4585Georgeo"/> and the <term key = "Dragon"/>, <ref type="authFile" corresp="AT1035HolyMan"></ref></desc></decoNote>
                    <decoNote type="miniature" xml:id="d05"><locus target="#35r"/><desc>The Madonna and Child, <ref type="authFile" corresp="AT1031VirginAndChild"></ref></desc></decoNote>
                    <decoNote type="miniature" xml:id="d06"><locus target="#54v"/><desc>The Flaggelation of Jesus <ref type = "authFile" corresp ="AT1015Flagellation"/></desc></decoNote>
                    <decoNote type="miniature" xml:id="d07"><locus target="#55r"/><desc>Taking off the Robe of Jesus <ref type = "authFile" corresp ="AT1093TakingRobeofJesus"/></desc></decoNote>
                    <decoNote type="miniature" xml:id="d08"><locus target="#66r"/><desc>The Sacrifice of Isaac <ref type = "authFile" corresp ="AT1092SacrificeIsaac"/></desc></decoNote>
                  </decoDesc>
                  <additions>
                    <list>
                      <item xml:id="a1">
                        <desc><locus target = "#iv"/> Unintelligible note</desc>
                      </item>
                      <item xml:id="a2">
                        <desc type = "ReceiptNote"><locus target = "#iv"/> Receipt of loan, written by <foreign xml:lang="gez">ባየ፡ አለሙ፡</foreign></desc>
                      </item>
                      <item xml:id="a3">
                        <desc><locus target = "#iiv"/> Hymn to Mary, “Rejoice you whom we beseech,” <foreign xml:lang="gez">ጸሎተ፡ ዕጣን።</foreign> Tənśa’e Zä-Guba’e Press, Addis Ababa, 1951 EC, pp. 26–7,
                        with incipit <foreign xml:lang="gez">ተፈሥሒ፡ ኦዘንስእለኪ፡ ዳኅና፡...</foreign>
                      </desc>
                      </item>
                      <item xml:id="a4">
                        <desc><locus target = "#iiir #iiiv"/> Acts 23:1–11, in a crude hand</desc>
                      </item>
                      <item xml:id="a5">
                        <desc><locus target = "#66v"/> Unintelligible note</desc>
                      </item>
                      <item xml:id="a6">
                        <desc type = "OwnershipNote">Note of ownership by <foreign xml:lang = "amh">ምስጋን፡ ውቤ፡</foreign>, in a crude hand, <locus target = "#65v"/></desc>
                      </item>

                      <item xml:id="e1">
                        <desc><locus target = "#ir #iir"/> blank, save for pen trial</desc>
                      </item>
                    </list>
                  </additions>
                  <bindingDesc>
                    <binding xml:id="binding" contemporary="true">
                      <decoNote xml:id="b1">Parchment, three of four Coptic chain stitches attached with bridle attachments to rough-hewn replacement boards (front repaired).</decoNote>
                      <decoNote xml:id="b2" type="bindingMaterial">
                        <material key="parchment"></material>
                        <material key="wood"></material>
                      </decoNote>
                    </binding>
                  </bindingDesc>
