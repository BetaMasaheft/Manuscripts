                     <msItem xml:id="ms_i1.4">
                        <locus from="160r" to="170v"/>
                        <title type="complete" ref="LIT2509Weddas"/>
                        <note>
                    Arranged for the days of the week (
                    Monday, <locus target="#160r"/>;
                    Tuesday, <locus target="#161r"/>;
                    Wednesday, <locus target="#163r"/>;
                    Thursday, <locus target="#164v"/>;
                    Friday, <locus target="#167r"/>;
                    Saturday, <locus target="#168v"/>;
                    Sunday, <locus target="#169v"/>)
                  </note>
                     </msItem>
                     <msItem xml:id="ms_i1.5">
