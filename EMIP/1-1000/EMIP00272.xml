                    <item xml:id="a2">
                    <desc type="OwnershipNote">
                    <locus target="#iir(ecto)"/>
                    pen trial and note of ownership, Aläqa A’ǝmǝro
                    </desc>
                    </item>
                    <item xml:id="e2">
