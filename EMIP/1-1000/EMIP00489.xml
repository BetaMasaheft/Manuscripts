               <msContents>
                  <summary>Psalter, ዳዊት፡</summary>

                  <msItem xml:id="ms_i1"><locus from="1r" to="103v" facs="001"/><title type="incomplete" ref="LIT2701Dawit"/>
                  <textLang mainLang="gez">Gǝ‘ǝz</textLang>
                  <note>Incomplete at the beginning and end. <ref type="work" corresp="LIT1113Anqasa"/> is missing</note>

                  <msItem xml:id="ms_i1.1"><locus from="1r" to="78v" facs="001"/><title type="incomplete" ref="LIT2000Mazmur"/>
                    <note>beginning at the end of <ref type="work" corresp="LIT2000Mazmur#Ps64"/></note>
                    <note><ref type="work" corresp="LIT2000Mazmur#Ps118Spiritual"/></note>
                  </msItem>
                  <msItem xml:id="ms_i1.2"><locus from="78v" to="93v" facs="079"/><title type="incomplete" ref="LIT1828Mahale"/>
                    <note>incomplete at the end (through the end of the thirteenth canticle)</note>
                  </msItem>
                  <msItem xml:id="ms_i1.3"><locus from="94r" to="102r" facs="094"/><title type="incomplete" ref="LIT2362Songof"/>
                    <note>incomplete at the beginning</note>
                  </msItem>
                  <msItem xml:id="ms_i1.4"><locus from="102r" to="103v" facs="102"/><title type="incomplete" ref="LIT2509Weddas"/>
                    <note>incomplete at the end</note>
                      <msItem xml:id="ms_i1.4.1"><locus from="102r" facs="102"/><title>Monday</title></msItem>
                      <msItem xml:id="ms_i1.4.2"><locus from="103r" facs="103"/><title>Tuesday</title></msItem>
                  </msItem>
                </msItem>
               </msContents>
