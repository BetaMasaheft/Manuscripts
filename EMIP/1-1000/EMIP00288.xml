                    <locus from="1r" to="163r" facs="008"/>
                    <title type="complete" ref="LIT2375Synaxa#FirstHalf"/>
                    <note>for the first half of the year</note>
                    <textLang mainLang="gez">Gǝ'ǝz</textLang>
