                        <q>“End of the month of Taḫśaś with the peace of God, Amen.”</q>
                      </explicit>
                    </msItem>
                      <msItem xml:id="ms_i1.5"><locus from="111r" to="141r" facs="228"/><title type="complete" ref="LIT2375Synaxa#Terr"/></msItem>
                      <msItem xml:id="ms_i1.6"><locus from="141v" to="163r" facs="289"/><title type="complete" ref="LIT2375Synaxa#Yakkatit"/>
