               <msContents>
                  <summary>Psalter, ዳዊት፡</summary>
                  <msItem xml:id="ms_i1">
                     <locus from="1r" to="158v"></locus>
                     <title type = "incomplete" ref="LIT2701Dawit"/>
                     <textLang mainLang="gez">Gə‘əz</textLang>

                     <msItem xml:id="ms_i1.1">
                       <locus from="1r" to="105r"></locus>
                       <title type = "incomplete" ref="LIT2000Mazmur"/>
                       <note>Begins with the end of Psalm 17.</note>
                     </msItem>

                     <msItem xml:id="ms_i1.2">
                       <locus from="105v" to="117r"></locus>
                       <title type = "complete" ref= "LIT1828Mahale"/>
                       <note>Contains marginal insertions from the Hebraic version.</note>
                     </msItem>

                     <msItem xml:id="ms_i1.3">
                       <locus from="117r" to="118v"></locus>
                       <title type = "incomplete" ref = "LIT2362Songof"/>
                       <note>Common version in the base text, with interlinear and marginal insertions from the Hebraic edition. Incomplete through the middle of part two.</note>
                     </msItem>
                  </msItem>
               </msContents>
