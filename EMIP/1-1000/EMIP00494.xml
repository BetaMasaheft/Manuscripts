                </msItem>
                  <msItem xml:id="ms_i3.4"><locus from="102r" to="103v"/><title type="incomplete" ref="LIT2509Weddas"/>
                  <textLang mainLang="gez">Gǝ‘ǝz</textLang>
                  <note>incomplete at the end. Arranged for the days of the week</note>
                <msItem xml:id="ms_i3.4.1"><locus from="102r"/><title>Monday</title>
                <textLang mainLang="gez">Gǝ‘ǝz</textLang>
                </msItem>
                  <msItem xml:id="ms_i3.4.2"><locus from="103r"/><title>Tuesday</title>
                  <textLang mainLang="gez">Gǝ‘ǝz</textLang>
                  </msItem>
                  </msItem>
              </msItem>
