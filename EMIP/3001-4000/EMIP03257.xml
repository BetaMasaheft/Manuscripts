               <msContents>
                  <summary>Chants Called Ziq for the Whole Year</summary>
                  <msItem xml:id="ms_i1">
                    <locus from="1r" to="16r" facs="001"/>
                    <title type="complete" ref="LIT2736RepCh20"></title>
                    <textLang mainLang="gez">Gǝ'ǝz</textLang>
                    <note>in a different hand</note>
                  </msItem>
                  <msItem xml:id="ms_i2">
                    <locus from="16r" to="160r" facs="016"/>
                    <title type="complete" ref="LIT1979Mashaf"></title>
                    <note><q>“which came from Gondar”</q> for the entire year (i.e., from Yohannes to Yohannes)</note>
                  <textLang mainLang="gez">Gǝ'ǝz</textLang>
                  <notatedMusic><desc>with musical notation</desc></notatedMusic>

                  <colophon xml:id="coloph1">
                    <locus target="#160v" facs="161"/>
                    repeating the same information as that in <ref type="item" corresp="#a4"/>
                  </colophon>
                </msItem>


