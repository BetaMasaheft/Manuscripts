                  <textLang mainLang="gez">Gǝ'ǝz</textLang>
                </msItem>
              </msItem>
                  <msItem xml:id="ms_i2"><locus from="157r" to="158v" facs="157"/><title type="complete" ref="LIT3108RepCh388">, written in a different hand</title>
