                  <msItem xml:id="ms_i1"><locus from="1r" to="5r" facs="001"/><title>Two sheets from a Psalter</title>
                  <textLang mainLang="gez">Gǝ'ǝz</textLang>
                   <msItem xml:id="ms_i1.1"><locus target="#1.Image1" facs="001"/><title>Sheet one: preserves <title type="complete" ref="LIT2000Mazmur#Chp57"/> (left) and <title type="complete" ref="LIT2000Mazmur#Chp60"/> (sic, the scribe wrote <foreign xml:lang="gez">፶፰</foreign>, right)</title>
                  <textLang mainLang="gez">Gǝ'ǝz</textLang>
                </msItem>
                     <msItem xml:id="ms_i1.2"><locus target="#2.Image2" facs="002"/><title>Sheet one: (the other side of the sheet in image 1) preserves <title type="complete" ref="LIT2000Mazmur#Chp61"/> (left) and <title type="complete" ref="LIT2000Mazmur#Chp56"/> (right)</title>
                  <textLang mainLang="gez">Gǝ'ǝz</textLang>
                </msItem>
                     <msItem xml:id="ms_i1.3"><locus target="#4.Image3" facs="004"/><title>Sheet two: preserves <title type="complete" ref="LIT2000Mazmur#Chp53"/> (left) and <title type="complete" ref="LIT2000Mazmur#Chp64"/> (right)</title>
