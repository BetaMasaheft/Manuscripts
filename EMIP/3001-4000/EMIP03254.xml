                    <decoNote type="miniature" xml:id="d7"><locus target="#111r" facs="111"/><desc><ref type="authFile" corresp="AT1038Baptism"/></desc></decoNote>
                    <decoNote type="miniature" xml:id="d8"><locus target="#135r" facs="135"/><desc>Conflicts with the Jews</desc></decoNote>
                    <decoNote type="miniature" xml:id="d9"><locus target="#135r" facs="135"/><desc><ref type="authFile" corresp="AT1005Crucifixion"/></desc></decoNote>
                    <decoNote type="miniature" xml:id="d10"><locus target="#135r" facs="135"/><desc><ref type="authFile" corresp ="AT1015Flagellation"/></desc></decoNote>
                    <decoNote type="miniature" xml:id="d11"><locus target="#135r" facs="135"/><desc><ref type="authFile" corresp ="AT1049Resurrection"/>, raising <persName ref="PRS1386Adam"/> and <persName ref="PRS3926Eve"/> from the dead</desc></decoNote>
