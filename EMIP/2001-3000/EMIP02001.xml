               <msContents>
                  <summary>Psalter, ዳዊት፡</summary>
                  <msItem xml:id="ms_i1">
                     <locus from="1r" to="160v"/>
                     <title type="complete" ref="LIT2701Dawit"/>
                     <textLang mainLang="gez">Gə‘əz</textLang>
                     <note>Gate of Light <locus target="#161"/> has been dislocated from its proper place after <locus target="#156"/>
                     </note>
                  </msItem>
               </msContents>
