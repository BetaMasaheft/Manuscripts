               <msContents>
                  <summary>Psalter</summary>
                <msItem xml:id="ms_i1">
                  <locus from="1r" to="194v" facs="003"/>
                  <title type="complete" ref="LIT2701Dawit"/>
                  <textLang mainLang="gez">Gǝ'ǝz</textLang>
                  <note>Psalm 118 does not contain the spiritual meanings of the Hebrew letters</note>

                <msItem xml:id="ms_i1.1">
                  <locus from="1r" to="158r" facs="003"/>
                  <title type="complete" ref="LIT2000Mazmur"/>
                </msItem>

                <msItem xml:id="ms_i1.2">
                <locus from="158r" to="177v" facs="160"/>
                <title type="complete" ref= "LIT1828Mahale"/>
                </msItem>

                <msItem xml:id="ms_i1.3">
                  <locus from="177v" to="185v" facs="180"/>
                  <title type="complete" ref="LIT2362Songof"/>
                </msItem>

                <msItem xml:id="ms_i1.4">
                  <locus from="186r" to="191v" facs="188"/>
                  <title type="incomplete" ref="LIT2509Weddas"/>

                <msItem xml:id="ms_i1.4.1">
                  <locus from="186r" facs="188"/>
                  <title>Thursday</title>
                  <note>incomplete at beginning</note>
                </msItem>

                <msItem xml:id="ms_i1.4.2">
                  <locus from="186r" facs="188"/>
                  <title>Friday</title>
                  <note>incomplete at the end</note>
                </msItem>

                <msItem xml:id="ms_i1.4.3">
                  <locus from="188r" facs="190"/>
                  <title>Saturday</title>
                  <note>incomplete at the beginning</note>
                </msItem>

                <msItem xml:id="ms_i1.4.4">
                  <locus from="189v" facs="192"/>
                  <title>Sunday</title>
                </msItem>
                </msItem>

                <msItem xml:id="ms_i1.5">
                  <locus from="191v" to="194v" facs="194"/>
                  <title type="incomplete" ref="LIT1113Anqasa"/>
                  <note>Incomplete at the end</note>
                </msItem>
                </msItem>

                </msContents>
