               <msContents>
                  <summary>Psalter</summary>
                <msItem xml:id="ms_i1">
                  <locus from="1r" to="132r" facs="003"/>
                  <title type="incomplete" ref="LIT2701Dawit"/>
                  <textLang mainLang="gez">Gǝ'ǝz</textLang>
                  <note>Praises of Mary, is missing</note>
                  <note><ref type="work" corresp="LIT2000Mazmur#Ps118Spiritual"/> does not contain the spiritual meanings of the Hebrew letters</note>
                </msItem>

                <msItem xml:id="ms_i1.1">
                  <locus from="1r" to="114r" facs="003"/>
                  <title type="incomplete" ref="LIT2000Mazmur"/>
                  <note>Incomplete at the beginning, starting at the end of Psalm 2. Many folios are missing between <locus target="#130v #131r" facs="133"/></note>
                </msItem>

                <msItem xml:id="ms_i1.2">
                  <locus from="114r" to="127v" facs="116"/>
                  <title type="complete" ref= "LIT1828Mahale"/>
                </msItem>

                <msItem xml:id="ms_i1.3">
                  <locus from="127v" to="130v" facs="130"/>
                  <title type="complete" ref="LIT2362Songof"/>
                </msItem>

