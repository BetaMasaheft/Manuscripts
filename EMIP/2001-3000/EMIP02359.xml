                  <decoNote type="miniature" xml:id="d5"><locus target="#57r" facs="059"/><desc><ref type="authFile" corresp="AT1031VirginAndChild"/> with two <term key="angel"/>s holding <term key="sword"/>s</desc></decoNote>
