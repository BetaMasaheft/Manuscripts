                  <decoNote type="miniature" xml:id="d8"><locus target="#58v"/><desc>Two <ref type="authFile" corresp ="AT1041HolyMen"/></desc></decoNote>
                  <decoNote type="miniature" xml:id="d9"><locus target="#81r"/><desc><ref type="authFile" corresp ="AT1099StMary"/> surrounded by six <term key="angel"/>s</desc></decoNote>
                  <decoNote type="miniature" xml:id="d10"><locus target="#81v"/><desc><ref type ="authFile" corresp="AT1035HolyMan"/> <name ref="PRS4306GabraMa"/> with lions and leopards</desc></decoNote>
                  <decoNote type="miniature" xml:id="d11"><locus target="#82r"/><desc><ref type="authFile" corresp="AT1005Crucifixion"/>, <persName ref="PRS6819Mary"/> and John stand beside the <term key="cross"/></desc></decoNote>
