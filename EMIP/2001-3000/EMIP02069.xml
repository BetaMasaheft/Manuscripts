                     <textLang mainLang="gez">Gə‘əz</textLang>
                     <note>Excerpt of Image of Gäbrä Mänfas Qəddus</note>
                  </msItem>
                  <msItem xml:id="ms_i5">
                     <locus from="23r" to="26r"/>
