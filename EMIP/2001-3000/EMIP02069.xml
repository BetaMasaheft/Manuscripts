                  </msItem>
                  <msItem xml:id="ms_i3">
                     <locus from="9r" to="20r"/>
                     <title type="complete" ref="LIT2260salota"/>
                     <textLang mainLang="gez">Gə‘əz</textLang>
                  </msItem>
                  <msItem xml:id="ms_i4">
                     <locus from="21r" to="22v"/>
