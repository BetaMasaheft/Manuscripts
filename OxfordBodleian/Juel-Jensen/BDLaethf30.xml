                        <msItem xml:id="ms_i1">
                      <locus from="1r" to="152vb"/>
                            <title ref="LIT2701Dawit" type="complete"/>
                            <textLang mainLang="gez"/>
                            
                            <msItem xml:id="ms_i1.1">
                                <locus from="1r" to="117v"/>
                                <title ref="LIT2000Mazmur" type="complete"/>
                                <textLang mainLang="gez"/>
                                <incipit xml:lang="gez">
                                    <locus target="#1r"/>
                                    <lb/><hi rend="rubric">ፍካሬ፡ በእንተ፡ ጻድቃን፡ ወኃጥአን፡ መዝሙር፡ ዘዳዊት፡</hi>
                                    <lb/>ብፁዕ፡ ብእሲ፡ ዘኢሖረ፡ በምክረ፡ ረሲዓን<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    <lb/> <hi rend="rubric">ወዘኢቆመ፡ ውስተ፡ ፍኖተ፡ ኃጥአን።</hi>
                                    <lb/>ወዘኢነበረ፡ ውስተ፡ መንበረ፡ መስተሳልቃን<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                </incipit>
                                <explicit xml:lang="gez">
                                    <locus from="117r" to="117v"/>
                                    <lb/>ወረገመኒ፡ በአማልክቲሁ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    <lb/>ወአንሰ፡ ነሣእኩ፡ ዘእምላዕሌሁ፡ ሰይፈ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    <pb n="117v"/><lb/>ወመተርኩ፡ ርእሶ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    <lb/>ወአሰሰልኩ፡ ፅዕለተ፡ እምደቂቀ፡ እስራኤል<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                </explicit>
                            </msItem>
                            
                            <msItem xml:id="ms_i1.2">
                                <!--<locus from="117v" to=""/>
                                images at end missing-->
                                <title type="complete" ref="LIT1828Mahale"/>
                                <msItem xml:id="ms_i1.2.1">
                                    <locus from="117v" to="118v"/>
                                    <title type="complete" ref="LIT1828Mahale#Moses1"/>
                                    <incipit xml:lang="gez">
                                        <locus target="#117v"/>
                                        <lb/><hi rend="rubric">ማኅልይ፡ ነቢያት፡ ወጸሎቱ፡ ለሙሴ።</hi>
                                        <lb/>ንሴብሖ፡ ለእግዚአብሔር፡ ስቡሐ፡ ዘተሰብሐ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ፈረሰ፡ ወመስተፅዕነ፡ ወረወ፡ ውስተ፡ ባሕር<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </incipit>
                                    <explicit xml:lang="gez">
                                        <locus target="#118v"/>
                                        ወአስተጋብአ፡ እግዚአብሔር፡ ላዕሌሆሙ፡ ማየ፡ ባሕር<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        ወደቂቀ፡ እስራኤልሰ፡ ኃለፉ፡ እንተ፡ የብስ፡ በማዕከለ፡ ባሕር<hi rend="rubric" rendition="#partialRubric">፨</hi>።
                                        ወማይሰ፡ ሎመ፡ አረፍት፡ በይምን፡ ወአረፍት፡ በጽግም<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </explicit>
                                </msItem>
                                <msItem xml:id="ms_i1.2.2">
                                    <locus from="118v" to="120r"/>
                                    <title type="complete" ref="LIT1828Mahale#Moses2"/>
                                    <incipit xml:lang="gez">
                                        <locus target="#118v"/>
                                        <lb/><hi rend="rubric">ዘሙሴ፡ ዘዳግም፡ ሕግ።
                                            <lb/>፪</hi> አጽምእ፡ ሰማይ፡ ወእንግርከ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ወትስማዕ፡ ምድር፡ ቃል፡ አፉየ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </incipit>
                                    <explicit xml:lang="gez">
                                        <locus target="#120r"/>
                                        <lb/>ወአምዕዑኒ፡ በአማልክቲሆሙ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ወአነ፡ አቀንዖሙ፡ በዘኢኮነ፡ ሕዝብ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ወአምዕዖሙ፡ ለሕዝብ፡ በዘኢይሌብው<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </explicit>
                                </msItem>
                                <msItem xml:id="ms_i1.2.3">
                                    <locus from="120r" to="121v"/>
                                    <title type="complete" ref="LIT1828Mahale#Moses3"/>
                                    <incipit xml:lang="gez">
                                        <locus target="#120r"/>
                                        <lb/><hi rend="rubric">ዘሙሴ፡ ዘሳልስ፡ ሕግ።
                                            <lb/>፫</hi> እስመ፡ እሳት፡ ትነድድ፡ እመዓትየ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ወታውዒ፡ እስከ፡ ሲኦል፡ ታሕተ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </incipit>
                                    <explicit xml:lang="gez">
                                        <locus target="#121v"/>
                                        <lb/>ይትፈደዮሙ፡ ወይትቤቀሎሙ፡ በቀለ፡ ለፀሩ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ወይትቤቀሎሙ፡ ለጻላእቱ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ወያነጽሕ፡ እግዚአብሔር፡ ምድረ፡ ለሕዝቡ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </explicit>
                                </msItem>
                                <msItem xml:id="ms_i1.2.4">
                                    <locus from="121v" to="122v"/>
                                    <title type="complete" ref="LIT1828Mahale#Hannah"/>
                                    <incipit xml:lang="gez">
                                        <locus target="#121v"/>
                                        <lb/><hi rend="rubric">ጸሎተ፡ ሐና፡ እመ፡ ሳሙኤል።
                                            <lb/>፬</hi> ጸንዓ፡ ልብየ፡ በእግዚአብሔር<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ወተለዓለ፡ ቀርንየ፡ በአምላኪየ፡ ወመድኃንየ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </incipit>
                                    <explicit xml:lang="gez">
                                        <locus target="#122v"/>
                                        <lb/>ወውእቱ፡ ጻድቅ፡ ይኰንን፡ እስከ፡ አጽናፈ፡ ምድር<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ወይሁቦሙ፡ ኃይለ፡ ለነገሥትነ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ወያሌዕል፡ ቀርነ፡ መሲሑ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </explicit>
                                </msItem>
                                <msItem xml:id="ms_i1.2.5">
                                    <locus from="122v" to="123r"/>
                                    <title type="complete" ref="LIT1828Mahale#Hezekiah"/>
                                    <incipit xml:lang="gez">
                                        <locus target="#122v"/>
                                        <lb/><hi rend="rubric">ጸሎተ፡</hi> ሕዝቅያስ። ። ነቢይ፡ ንጉሠ፡ ይሁዳ።
                                        <lb/><hi rend="rubric">፭</hi> አንሰ፡ እቤ፡ በኑኃ፡ መዋዕልየ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ውስተ፡ አናቅፀ፡ ሲኦል፡ ኀደጉ፡ ዘተረፈ፡ ዓመትየ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </incipit>
                                    <explicit xml:lang="gez">
                                        <locus target="#123r"/>
                                        <lb/>እነግር፡ ጽድቀከ፡ በዳኅናየ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ወትረ፡ እሴብሐከ፡ ወእዜምር፡ ለከ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>በኵሉ፡ መዋዕለ፡ ሕይወትየ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>በቅድመ፡ ቤተ፡ እግዚአብሔር<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </explicit>
                                </msItem>
                                <msItem xml:id="ms_i1.2.6">
                                    <locus from="123r" to="124r"/>
                                    <title type="complete" ref="LIT1828Mahale#Manasseh"/>
                                    <incipit xml:lang="gez">
                                        <locus target="#123r"/>
                                        <lb/><hi rend="rubric">ጸሎተ፡ ምናሴ፡ ነቢይ።
                                            <lb/>፮፡</hi> እግዚኦ፡ አኀዜ፡ ኵሉ፡ ዓለም<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>አምላኮሙ፡ ለአበዊነ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ለአብርሃም፡ ወለይስሐቅ፡ ወለያዕቆብ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </incipit>
                                    <explicit xml:lang="gez">
                                        <locus target="#124r"/>
                                        <lb/>እሴብሐከ፡ በኵሉ፡ ጊዜ፡ ወበኵሉ፡ መዋዕለ፡ ሐይወትየ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>እስመ፡ ኪያከ፡ ይሴብሑ፡ ኵሉ፡ ኃይለ፡ ሰማያት<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ወለከ፡ ሰብሐት፡ ለዓለመ፡ ዓለም፡ አሜን<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </explicit>
                                </msItem>
                                <msItem xml:id="ms_i1.2.7">
                                    <locus from="124r" to="124v"/>
                                    <title type="complete" ref="LIT1828Mahale#Jonah"/>
                                    <incipit xml:lang="gez">
                                        <locus target="#124r"/>
                                        <lb/><hi rend="rubric">ጸሎተ፡ ዮናስ፡ ነቢይ።
                                            <lb/>፯</hi> ጸራኅኩ፡ በምንዳቤየ፡ ኀበ፡ እግዚአብሔር፡ አምላኪየ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ወሰምዓኒ፡ በውስተ፡ ከርሠ፡ ሲኦል<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </incipit>
                                    <explicit xml:lang="gez">
                                        <locus target="#124v"/>
                                        <lb/>ወእስከ፡ አቀቡ፡ ከንቶ፡ ወሐሰተ፡ ገደፉ፡ ሣህሎ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ወአንሰ፡ ምስለ፡ ቃለ፡ ሰብሐት፡ በተጋንዮ፡ እሰውዕ፡ ለከ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>መጠነ፡ ጸለይኩ፡ በሕይወትየ፡ አአስዮ፡ ለእግዚአብሔር<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </explicit>
                                </msItem>
                                <msItem xml:id="ms_i1.2.8">
                                    <locus from="124v" to="125v"/>
                                    <title type="complete" ref="LIT1828Mahale#ThreeYouths1"/>
                                    <incipit xml:lang="gez">
                                        <locus target="#124v"/>
                                        <lb/><hi rend="rubric">ጸሎተ፡ ዳንኤል፡ ነቢይ፡ ስብሐት፡ ዘእምአዛርያ።</hi>
                                        <lb/>ይትባረክ፡ እግዚአብሔር፡ አምላከ፡ አበዊነ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/> እኵት፡ ወስቡሕ፡ ስምከ፡ ለዓለም<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </incipit>
                                    <explicit xml:lang="gez">
                                        <locus target="#125v"/>
                                        <lb/> ይትኃፈሩ፡ በኵሉ፡ ትእግልቶሙ፡ ወይትቀጥቅጥ፡ ኃይሎሙ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/> ወያእምሩ፡ ከመ፡ አንተ፡ እግዚአብሔር፡ አምላክ፡ ባሕቲትከ፡ ልዑል<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/> ስቡሕ፡ በኵሉ፡ ምድር፡ ወበኵሉ፡ ዓለም<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </explicit>
                                </msItem>
                                <msItem xml:id="ms_i1.2.9">
                                    <locus from="125v" to="126r"/>
                                    <title type="complete" ref="LIT1828Mahale#ThreeYouths2"/>
                                    <incipit xml:lang="gez">
                                        <locus target="#125v"/>
                                        <lb/><hi rend="rubric">ጸሎተ፡ ፫ ደቂቅ።
                                            <lb/>፱</hi> ይትባረክ፡ እግዚአብሔር፡ አምላከ፡ አበዊነ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/> ስቡሕኒ፡ ውእቱ፡ ወልዑልኒ፡ ውእቱ፡ ለዓለም<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </incipit>
                                    <explicit xml:lang="gez">
                                        <locus from="125v" to="126r"/>
                                        <lb/>ስቡሕኒ፡ አንተ፡ ወቡሩክ፡ አንተ፡ ለዓለም<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <lb/>ቡሩክ፡ አንተ፡ በመልዕልተ፡ ሰማያት<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                        <pb n="126r"/><lb/>ስቡሕኒ፡ አንተ፡ ወክቡር፡ አንተ፡ ለዓለም<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                    </explicit>
                                </msItem>
                                <msItem xml:id="ms_i1.2.10">
                                    <locus from="126r" to="127r"/>
                                    <title type="complete" ref="LIT1828Mahale#ThreeYouths3"/>
                                    <incipit xml:lang="gez">
                                        <locus target="#126r"/>
                                        <lb/><hi rend="rubric">ኀበ፡ ባረኩ፡ አናንያ፡ ወአዛርያ፡ ወሚሳኤል። 
                                            <lb/>፲ይ</hi> ባ<hi rend="rubric">ር</hi>ክዎ፡ ኵሉ፡ ግብረ፡ እግዚእ፡ ለእግዚአብሔር<hi rend="rubric" rendition="#partialRubric">፨</hi>
