                        <msItem xml:id="p3_i2">
                            <locus from="41r" to="43v"/>
                            <title ref="NAR0005calend" type="complete"/>
                            <textLang mainLang="gez"/>
                            <incipit xml:lang="gez">
                                <locus target="#41r"/>
