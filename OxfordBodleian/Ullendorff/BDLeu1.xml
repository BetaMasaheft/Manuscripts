                        <msItem xml:id="ms_i1">
                            <locus from="1r" to="138v"/>
                            <title type="complete" ref="LIT2000Mazmur"/>
                            <textLang mainLang="gez"/>
                            <incipit xml:lang="gez">
                                <locus target="#1r"/>
                                    <lb/><hi rend="rubric"><gap reason="lost" unit="chars" quantity="1"/>ካሬ፡ ዘጻድቃን፡ ወኃጥአን፡ መዝሙር፡ ዘዳዊት፡</hi>
                                <lb/><hi rend="rubric">፩</hi> ብፁዕ፡ ብእሲ፡ ዘኢሖረ፡ በምክረ፡ ረሲአን<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                <lb/><hi rend="rubric">ወዘኢቆመ፡ ውስተ፡ ፍኖተ፡ ኃጥአን።</hi>
                                <lb/>ወዘኢነበረ፡ ውስተ፡ መንበረ፡ መስተሣልቃን<hi rend="rubric" rendition="#partialRubric">፨</hi>
                            </incipit>
                            <explicit xml:lang="gez">
                                <locus target="#138v"/>
                                <lb/>ወረገመኒ፡ በአማልክቲሁ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                <lb/>ወአንሰ፡ ነሣእኩ፡ ዘእምላዕሌሁ፡ ሰይፈ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                <lb/>ወመተርኩ፡ ርእሶ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                <lb/>ወአሰሰልኩ፡ ጽእለተ፡ እምደቂቀ፡ እስራኤል<hi rend="rubric" rendition="#partialRubric">፨</hi>
                            </explicit>
                        </msItem>
                        
                        <msItem xml:id="ms_i2">
                            <locus from="139r" to="154v"/>
                            <title type="complete" ref="LIT1828Mahale"/>
                            <textLang mainLang="gez"/>
                            <incipit xml:lang="gez">
                                <locus target="#139r"/>
                                <lb/><hi rend="rubric">ማኅልየ፡ ነቢያት፡ ወጸሎቱ፡ ለሙሴ፡ <del rend="erasure"/></hi>
                                <lb/><hi rend="rubric">፩</hi> ንሴብሖ፡ ለእግዚአብሔር፡ <sic>ስቡሐ፡</sic> ዘተሰብሐ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                <lb/><hi rend="rubric">ፈረሰ፡ ወመስተፅዕነ፡ ወረወ፡ ውስተ፡ ባሕር።</hi>
                            </incipit>
                            <explicit xml:lang="gez">
                                <locus target="#154v"/>
                                <lb/>ዘአስተዳሎከ፡ ቅድመ፡ ኵሉ፡ ሕዝብከ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                <lb/>ከመ፡ ትክሥት፡ ብርሃነ፡ ለአሕዛብ<hi rend="rubric" rendition="#partialRubric">፨</hi>
                                <lb/>ወክብረ፡ ለሕዝብከ፡ እስራኤል<hi rend="rubric" rendition="#partialRubric">፨</hi>
                            </explicit>
                          
                           <!-- <msItem xml:id="ms_i2.1">
                                <locus from="139r" to="140r"/>
                                <title type="complete" ref="LIT1828Mahale#Moses1"/>
                                <incipit xml:lang="gez">
                                    <locus target="#139r"/>
                                    <lb/><hi rend="rubric">ማኅልየ፡ ነቢያት፡ ወጸሎቱ፡ ለሙሴ፡ <del rend="erasure"/></hi>
                                    <lb/><hi rend="rubric">፩</hi> ንሴብሖ፡ ለእግዚአብሔር፡ <sic>ስቡሐ፡</sic> ዘተሰብሐ፨
                                    <lb/><hi rend="rubric">ፈረሰ፡ ወመስተፅዕነ፡ ወረወ፡ ውስተ፡ ባሕር።</hi>
                                </incipit>
                                <explicit xml:lang="gez">
                                    <locus target="#140r"/>
                                    <lb/>ወአስተጋብአ፡ እግዚአብሔር፡ ላዕሌሆሙ፡ ማየ፡ ባሕር፨
                                    <lb/>ወደቂቀ፡ እስራኤልሰ፡ ኃለፉ፡ እንተ፡ የብስ፡ ማእከለ፡ ባሕር፨
                                    <lb/>ወማይሰ፡ ቆመ፡ ሎመ፡ አረፍት፡ በይምን፡ ወአረፍት፡ በጽግም፨
                                </explicit>
                            </msItem>
                            
                            <msItem xml:id="ms_i2.2">
                                <locus from="140r" to=""/>
                                <title type="complete" ref="LIT1828Mahale#Moses2"/>
                                <incipit xml:lang="gez">
                                    <locus target="#140r"/>
                                    <lb/><hi rend="rubric">ዘሙሴ፡ ዘዳግም፡ ሕግ።
                                        <lb/>፪።</hi> አጽምዕ፡ ሰማይ፡ ወእንግርከ፨
                                    <lb/>ወትስማዕ፡ ምድር፡ ቃለ፡ አፉየ፨
                                </incipit>
                            </msItem>
                            
                        <msItem xml:id="ms_i2.3">
                                <locus from="" to=""/>
                                <title type="complete" ref="LIT1828Mahale#Moses3"/>
                          <!-\-      <incipit xml:lang="gez">
                                    <locus target="#109r"/>
                                    <lb/><hi rend="rubric">ጸሎተ፡ ሙሴ፡ ዘሣልስ፡ ሕግ።
                                        <lb/>፫</hi> እስመ፡ እሳት፡ ትነድድ፡ እመዐትየ።
                                    <lb/>ወታውዒ፡ እስከ፡ ሲኦል፡ ታሕተ።
                                </incipit>
                                <explicit xml:lang="gez">
                                    <locus target="#110v"/>
                                    <lb/>ይትቤቀል፡ ወይትፈደይ፡ በቀለ፡ ለፀሩ፡
                                    <lb/>ወይትቤቀሎሙ፡ ለፀላእቱ።
                                    <lb/>ወያነጽሕ፡ <hi rend="rubric">እግዚአብሔር፡</hi> ምድረ፡ ለሕዝቡ።
                                </explicit>-\->
                            </msItem>
                            <msItem xml:id="ms_i2.4">
                                <locus from="144r" to="144v"/>
                                <title type="complete" ref="LIT1828Mahale#Hannah"/>
                          <!-\-      <incipit xml:lang="gez">
                                    <locus target="#110v"/>
                                    <lb/><hi rend="rubric">ጸሎተ፡ ሐና፡ እመ፡ ሳሙኤል፡ ነቢይ።
                                        <lb/>፬፡</hi> ጸንዓ፡ ልብየ፡ በ<hi rend="rubric">እግዚአብሔር</hi>።
                                    <lb/>ወተለዓለ፡ ቀርንየ፡ በአምላኪየ፡ ወመድኃንየ።
                                </incipit>-\->
                                <explicit xml:lang="gez">
                                    <locus target="#144v"/>
                                    <lb/>ወውእቱ፡ ጻድቅ፡ ይኴንን፡ እስከ፡ አጽናፈ፡ ምድር፨
                                    <lb/>ወይሁቦሙ፡ ኃይለ፡ ለነገሥት። ወያሌዕል፡ ቀርነ፡ መሢሑ፨
                                </explicit>
                            </msItem>
                            <msItem xml:id="ms_i2.5">
                                <locus from="145r" to=""/>
                                <title type="complete" ref="LIT1828Mahale#Hezekiah"/>
                                <incipit xml:lang="gez">
                                    <locus from="145r"/>
                                    <lb/><hi rend="rubric">ጸሎተ፡ ሕዝቅያስ፡ ንጉሠ፡ ይሁዳ።
                                        <lb/>፭፡</hi> አንሰ፡ እቤ፡ በኑኃ፡ መዋዕልየ።
                                    <lb/>ውስተ፡ አናቅጸ፡ ሲኦል፡ ኀደጉ፡ ዘተረፈ፡ ዓመትየ።
                                </incipit>
                            <!-\-    <explicit xml:lang="gez">
                                    <locus target="#112r"/>
                                    <lb/>እነግር፡ ጽድቀከ፡ በዳኅናየ።
                                    <lb/>ወትረ፡ እሴብሐከ፡ ወእዜምር፡ ለከ።
                                    <lb/>በኵሉ፡ መዋዕለ፡ ሕይወትየ። በቅድመ፡ ቤተ፡ <hi rend="rubric">እግዚ<seg rend="above">አብሔር።</seg></hi>
                                </explicit>-\->
                            </msItem>
                            <msItem xml:id="ms_i2.6">
                                <locus from="145v" to=""/>
                                <title type="complete" ref="LIT1828Mahale#Manasseh"/>
                               <!-\- <incipit xml:lang="gez">
                                    <locus target="#112r"/>
                                    <lb/><hi rend="rubric">ጸሎተ፡ ምናሴ፡ ነቢይ።
                                        <lb/>፮፡</hi> እግዚኦ፡ አኃዜ፡ ኵሉ፡ ዓለም።
                                    <lb/>አምላኮሙ፡ ለአበዊነ። ለአብርሃም፡ ወለይስሐቅ፡ <seg rend="above">ወለያዕቆብ።</seg>
                                </incipit>
                                <explicit xml:lang="gez">
                                    <locus from="112v" to="113r"/>
                                    <lb/>እሴብሐከ፡ በኵሉ፡ ጊዜ፡ ወበኵሉ፡ መዋዕለ፡ ሐይወት<seg rend="below">የ።</seg>
                                    <lb/>እስመ፡ ኪያከ፡ ይሴብሑ፡ ኵሉ፡ ኃይለ፡ ሰማያት። <pb n="113r"/>
                                    <lb/>ወለከ፡ ሰብሐት፡ ለዓለመ፡ ዓለም።
                                </explicit>-\->
                            </msItem>
                            <msItem xml:id="ms_i2.7">
                                <locus from="147r" to=""/>
                                <title type="complete" ref="LIT1828Mahale#Jonah"/>
                         <!-\-       <incipit xml:lang="gez">
                                    <locus target="#113r"/>
                                    <lb/><hi rend="rubric">ጸሎተ፡ ዮናስ፡ ነቢይ።
                                        <lb/>፯፡</hi> ጸራኅኩ፡ በምንዳቤየ፡ ኀበ፡ <hi rend="rubric">እግዚአብሔር፡ አ<seg rend="above">ምላኪየ።</seg></hi>
                                    <lb/>ወሰምዓኒ፡ <sic resp="DR">በወለተ፡</sic> ከርሠ፡ ሲኦል።
                                </incipit>
                                <explicit xml:lang="gez">
                                    <locus target="#113r"/>
                                    <lb/>እስከ፡ ዓቀቡ፡ ከንቶ፡ ወሐሰተ፡ ገደፉ፡ ሣህሎሙ።
                                    <lb/>አንሰ፡ ምስለ፡ ቃለ፡ ሰብሐት፡ በተጋንዮ፡ እሠውዕ፡ ለከ።
                                    <lb/>መጠነ፡ ጸለይኩ፡ በሕይወትየ፡ አዓሥዮ፡ ለ<hi rend="rubric">እግዚአ<seg rend="below">ብሔር።</seg></hi>
                                </explicit>-\->
                            </msItem>
                            <msItem xml:id="ms_i2.8">
                                <locus from="147v" to="149r"/>
                                <title type="complete" ref="LIT1828Mahale#ThreeYouths1"/>
                            <!-\-    <incipit xml:lang="gez">
                                    <locus target="#113r"/>
                                    <lb/><hi rend="rubric">ጸሎተ፡ ዳንኤል፡ ነቢይ፡ ስብሐት፡ ዘእምአ<seg rend="above">ዛርያ።</seg>
                                        <lb/> ፰</hi> ይትባረክ፡ <hi rend="rubric">እግዚአብሔር፡</hi> አምላከ፡ አበዊነ።
                                    <lb/> እኵት፡ ወስቡሕ፡ ስምከ፡ ለዓለም።
                                </incipit>-\->
                                <explicit xml:lang="gez">
                                    <locus from="148v" to="149r"/>
                                    <lb/> ይትኃፈሩ፡ ኵሎሙ፡ እለ፡ ይሰግዱ፡ ለግልፎ፨
                                    <lb/> ወያእምሩ፡ ከመ፡ አንተ፡ እግዚአብሔር፡ አምላክ፡ ባሕቲትከ፨
                                    <pb n="149r"/>
                                    <lb/> ስቡሕ፡ በኵሉ፡ ምድር፡ ወበኵሉ፡ ዓለም፨
                                </explicit>
                            </msItem>
                            <msItem xml:id="ms_i2.9">
                                <locus target="#149r"/>
                                <title type="complete" ref="LIT1828Mahale#ThreeYouths2"/>
                                <incipit xml:lang="gez">
                                    <locus target="#149r"/>
                                    <lb/><hi rend="rubric">ጸሎተ፡ ፫ ደቂቅ።
                                        <lb/>፱</hi> ይትባረክ፡ እግዚአብሔር፡ አምላከ፡ አበዊነ፨
                                    <lb/>ስቡሕኒ፡ ውእቱ፡ ወልዑልኒ፡ ውእቱ፡ ለዓለም፨
                                </incipit>
                                <explicit xml:lang="gez">
                                    <locus target="#149r"/>
                                    <lb/>ቡሩክ፡ አንተ፡ በዲበ፡ መንበረ፡ መንግሥትከ፨
                                    <lb/><add place="overstrike">ቡሩክ፡</add> አንተ፡ በመልዕልተ፡ ሰማያት፨
                                    <lb/>ስቡሕኒ፡ አንተ፡ ወክቡርኒ፡ አንተ፡ ለዓለም፨
                                </explicit>
                            </msItem>
                            <msItem xml:id="ms_i2.10">
                                <locus from="149r" to=""/>
                                <title type="complete" ref="LIT1828Mahale#ThreeYouths3"/>
                                <incipit xml:lang="gez">
                                    <locus target="#149r"/>
                                    <lb/><hi rend="rubric">ኀበ፡ ባረኩ፡ አናንያ፡ <add place="inline">ወ</add>አዛርያ፡ ወሚሳኤል፡ ለእግዚአብሔር።
                                        <lb/>፲</hi> ይባርክዎ፡ ኵሉ፡ ግብረ፡ እግዚእ፡ ለእግዚአብሔር፨
                                    <lb/>ስቡሕኒ፡ ውእቱ፡ ወልዑልኒ፡ ውእቱ፡ ለዓለም፨
                                </incipit>
                            <!-\-    <explicit xml:lang="gez">
                                    <locus target="#115r"/>
                                    <lb/>ይባርክዎ፡ አናንያ፡ ወአዛርያ፡ ወሚሳኤል፡ ለእ<supplied reason="omitted">ግዚአብሔር።</supplied>
                                    <lb/>ንባርኮ፡ ለአብ፡ ወወልድ፡ ወመንፈስ፡ ቅዱስ።
                                    <lb/>ስቡሕኒ፡ ውእቱ፡ ወልዑልኒ፡ ውእቱ፡ ለዓለም።
