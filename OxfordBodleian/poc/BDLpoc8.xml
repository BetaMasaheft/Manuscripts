                                <title type="complete" ref="LIT2001Mazmur"/>
                                <textLang mainLang="gez"/>
                                
                                <note>This manuscript was used by <persName ref="PRS6378Ludolf"/> to compile his calendar in 
                                    <bibl>
                              <ptr target="bm:Ludolf1691Commentarius"/>
                              <citedRange unit="page">389–427</citedRange>
