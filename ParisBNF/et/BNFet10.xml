                        <item xml:id="a1">
                           <desc type="GuestText">
                              <title type="complete" ref="LIT3308Nicene"/> This text been added
                              later at the end of the volume. It differs somewhat from the mass
                              canon <bibl>
                                 <ptr target="bm:Ludolf1691Commentarius"/>
                                 <citedRange unit="page">352–353</citedRange>
