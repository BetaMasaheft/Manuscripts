                     <handNote xml:id="h1" script="Ethiopic">
                        <ab type="script" part="N">Hand a: mediocre, irregular; hand b: well trained</ab>
                        <seg type="ink" part="N">Black, red (strong crimson for the regular rubrication; vivid scarlet for the names of the donors)</seg>
                        <persName role="scribe" corresp="PRS00000" instant="false" full="yes"><!--Only the name of the scribe should be here, with appropriate correspondence--></persName>
